{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "typeRoots": ["./node_modules/@types", "./src/types"]
    },
    "include": ["src/**/*.ts", "src/types/express.d.ts"]
}
